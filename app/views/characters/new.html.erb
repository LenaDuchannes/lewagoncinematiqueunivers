<section class="jumbotron text-center new-core">
  <div class="container">
    <%= simple_form_for @character do |c| %>
      <%= c.input :name %>
      <%= c.input :photo %>
      <%= c.input :photo_cache, as: :hidden %>
      <%= c.full_error :token %>
      <%= c.submit 'Save' %>
    <% end %>
  </div>
</section>
